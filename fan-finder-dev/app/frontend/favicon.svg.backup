<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 100 100" width="100" height="100">
  <defs>
    <linearGradient id="robotGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#ff6b35;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#f7931e;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Robot Head -->
  <rect x="25" y="25" width="50" height="40" rx="8" ry="8" fill="url(#robotGradient)" stroke="#333" stroke-width="2"/>
  
  <!-- Robot Eyes -->
  <circle cx="35" cy="40" r="4" fill="#fff"/>
  <circle cx="65" cy="40" r="4" fill="#fff"/>
  <circle cx="35" cy="40" r="2" fill="#333"/>
  <circle cx="65" cy="40" r="2" fill="#333"/>
  
  <!-- Robot Mouth -->
  <rect x="42" y="52" width="16" height="6" rx="3" ry="3" fill="#333"/>
  
  <!-- Robot Antennas -->
  <line x1="35" y1="25" x2="35" y2="15" stroke="#333" stroke-width="2"/>
  <line x1="65" y1="25" x2="65" y2="15" stroke="#333" stroke-width="2"/>
  <circle cx="35" cy="15" r="3" fill="#ff6b35"/>
  <circle cx="65" cy="15" r="3" fill="#ff6b35"/>
  
  <!-- Robot Body -->
  <rect x="30" y="65" width="40" height="25" rx="5" ry="5" fill="url(#robotGradient)" stroke="#333" stroke-width="2"/>
  
  <!-- Robot Arms -->
  <rect x="15" y="70" width="15" height="8" rx="4" ry="4" fill="url(#robotGradient)" stroke="#333" stroke-width="1"/>
  <rect x="70" y="70" width="15" height="8" rx="4" ry="4" fill="url(#robotGradient)" stroke="#333" stroke-width="1"/>
</svg>